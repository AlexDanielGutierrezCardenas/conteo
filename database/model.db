drop table conteovotos;
drop table candidato;
drop table mesas;
drop table recinto;
drop table municipio_departamento;
drop table municipio;

CREATE TABLE municipio (
    id SERIAL PRIMARY KEY,
    nombre VARCHAR(255) NOT NULL,
    numero INT NOT NULL,
    nombre_municipio VARCHAR(255) NOT NULL,
    departamento VARCHAR(255) NOT NULL
);

CREATE TABLE municipio_departamento (
    id SERIAL PRIMARY KEY,
    id_municipio INT NOT NULL,
    num_distrito INT NOT NULL,
    zona VARCHAR(255) NOT NULL,
    foreign key (id_municipio) references municipio(id)
);

create table recinto(
    id SERIAL PRIMARY KEY,
    nombre VARCHAR(255) NOT NULL,
    descripcion VARCHAR(255) NOT NULL,
    id_municipio_departamento INT NOT NULL,
    foreign key (id_municipio_departamento) references municipio_departamento(id)
);

create table mesas(
    id SERIAL PRIMARY KEY,
    numero_mesa INT NOT NULL,
    numero_electores INT NOT NULL,
    num_certificicado INT NOT NULL,
    num_votantes INT NOT NULL,
    numero_votos_blancos INT NOT NULL,
    numero_votos_nulos INT NOT NULL,
    id_recinto INT NOT NULL,
    foreign key (id_recinto) references recinto(id)
);

create table candidato(
    id SERIAL PRIMARY KEY,
    nombre_completo VARCHAR(255) NOT NULL,
    partido VARCHAR(255) NOT NULL,
    propuesta VARCHAR(255) NOT NULL
);

create table conteovotos(
    id SERIAL PRIMARY KEY,
    id_candidato INT NOT NULL,
    id_mesa INT NOT NULL,
    num_voto INT NOT NULL,
    foreign key (id_candidato) references candidato(id),
    foreign key (id_mesa) references mesas(id)
);
